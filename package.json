{
  "name": "uab-ui",
  "version": "1.0.2",
  "description": "A UI components to help developers grow",
  "author": "shunyue1320",
  "main": "dist/uab-ui.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name uab-ui src/index.js",
    "test": "parcel build tests/* --no-cache --no-minify && karma start --single-run",
    "test-dev": "parcel watch tests/* --no-cache & karma start",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "deploy": "bash deploy.sh"
  },
  "dependencies": {
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-service": "^4.5.8",
    "@vue/component-compiler-utils": "^3.2.0",
    "chai": "^4.2.0",
    "chai-spies": "^1.0.0",
    "karma": "^5.2.3",
    "karma-chai": "^0.1.0",
    "karma-chai-spies": "^0.1.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-sinon-chai": "^2.0.2",
    "mocha": "^8.2.0",
    "parcel-bundler": "^1.12.4",
    "node-sass": "^5.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "sinon": "^9.2.1",
    "sinon-chai": "^3.5.0",
    "vue-template-compiler": "^2.6.12",
    "vuepress": "^1.5.3"
  },
  "alias": {
    "vue": "./node_modules/vue/dist/vue.common.js"
  },
  "bugs": {
    "url": "https://github.com/shunyue1320/uab-ui/issues"
